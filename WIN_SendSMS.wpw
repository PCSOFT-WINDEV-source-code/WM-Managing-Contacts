   -
     name : FillNumbersAccordingToContact
     procedure_id : 1664247806572687296
     type_code : 14
     code : |1+
      // Summary: Fills the combo box of numbers
      // Syntax:
      //FillNumbersAccordingToContact ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      PROCEDURE FillNumbersAccordingToContact()
      
      // Deletes the former numbers
      ListDeleteAll(COMBO_Numbers)
      
      FOR EACH tPhone of gContact.phone
      	ListAdd(COMBO_Numbers,tPhone..Number)
      END
      
      IF COMBO_Numbers..Occurrence > 0 THEN
      	COMBO_Numbers = 1
      ELSE
      	Error("This contact has no phone number")
      	Close()
      END
      
     type : 458752
