        code : |1+
         
         IF COMBO_Numbers = "" THEN
         	Error("Enter a phone number.")
         	RETURN
         	
         ELSE
         	NextTitle("Sending the SMS...")
         	
         	//Reset all the members of the Email structure to 0
         	SMSReset()
         	
         	SMS.Message = EDT_MESSAGE
         	SMS.Number = COMBO_Numbers[COMBO_Numbers]
         	
         	//Send the SMS
         
         <SI CibleExécution=Android>
         		
         		IF NOT SMSSend() THEN
         			HourGlass(False)
         			Error("This SMS has not been sent.")
         		ELSE
         			HourGlass(False)
         			ToastDisplay("The SMS was successfully sent.")
         			Close()
         	END
         	
         <SINON SI CibleExécution=iOS>
         	
         	
         	SMSRunApp()
         	Close()
         	
         <FIN>
         
         	
         	
         	
         	
         END
         
